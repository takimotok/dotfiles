{
  "php.version": "7.0.2",
  "language_server_phpstan.enabled": false,
  "language_server_php_cs_fixer.enabled": true,
  "language_server_php.formatting": false,
  "diagnostics.enabled": true,
  "symfony.enabled": true,
  "phpunit.enabled": true,
  "indexer.exclude_patterns": [
    "/vendor/**/Tests/**/*",
    "/vendor/**/tests/**/*",
    "/var/cache/**/*"
  ],
  "completion.enable": true,
  "prophecy.enabled": true,
  "language_server_worse_reflection.diagnostics": {
    "disabled": [
      {
        "name": "worse.missing_return_type",
        "paths": ["/tests/**/*"]
      },
      {
        "name": "worse.docblock_missing_return_type",
        "paths": ["/tests/**/*"]
      }
    ]
  }
}

